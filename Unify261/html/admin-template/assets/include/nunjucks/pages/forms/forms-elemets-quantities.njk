<main>
	{% include "html/admin-template/assets/include/nunjucks/partials/blocks/headers/header-fixed.njk" %}

	<section class="container-fluid px-0 g-pt-65">
		<div class="row no-gutters g-pos-rel g-overflow-x-hidden">
			{% include "html/admin-template/assets/include/nunjucks/partials/blocks/sidebars/sidebar-static.njk" %}

			<div class="col g-ml-45 g-ml-0--lg g-pb-65--md">
				{% include "html/admin-template/assets/include/nunjucks/partials/blocks/breadcrumbs/breadcrumb-v1.njk" %}

				<div class="g-pa-20">
					<h1 class="g-font-weight-300 g-font-size-28 g-color-black g-mb-28">{{ title }}</h1>

					<div class="row">
						<!-- 1-st column -->
						<div class="col-md-4">
							<!-- Basic Quantities -->
              <div class="g-brd-around g-brd-gray-light-v7 g-rounded-4 g-pa-15 g-pa-20--md g-mb-30">
                <h3 class="d-flex align-self-center text-uppercase g-font-size-12 g-font-size-default--md g-color-black g-mb-20">Basic Quantities</h3>

                <!-- Basic v.1 -->
                <div class="g-mb-30">
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Basic v.1</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-185 g-brd-gray-light-v7 g-rounded-4">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-right g-brd-gray-light-v7">
                        <i class="hs-admin-minus"></i>
                      </div>

                      <div class="input-group-addon d-flex justify-content-center align-items-center g-width-40 g-font-size-16 g-color-gray-light-v11 g-bg-gray-light-v8 rounded-0">
                        <span>$</span>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 g-brd-left g-brd-gray-light-v7 rounded-0 g-pa-9" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-left g-brd-gray-light-v7">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Basic v.1 -->

                <!-- Basic v.2 -->
                <div class="g-mb-30">
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Basic v.2</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-140 g-brd-gray-light-v7 g-rounded-4">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-right g-brd-gray-light-v7">
                        <i class="hs-admin-minus"></i>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 rounded-0 g-pa-9" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-left g-brd-gray-light-v7">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Basic v.2 -->

                <!-- Basic v.3 -->
                <div>
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Basic v.3</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-110 g-brd-none">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-30 g-height-30 g-font-size-10 g-color-gray-light-v11 g-brd-around g-brd-gray-light-v7 g-rounded-4 g-pa-5">
                        <i class="hs-admin-minus"></i>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 rounded-0 p-0" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-30 g-height-30 g-font-size-10 g-color-gray-light-v11 g-brd-around g-brd-gray-light-v7 g-rounded-4 g-pa-5">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Basic v.3 -->
              </div>
							<!-- End Basic Quantities -->
						</div>
						<!-- End 1-st column -->

            <!-- 2-nd column -->
            <div class="col-md-4">
              <!-- Round Quantities -->
              <div class="g-brd-around g-brd-gray-light-v7 g-rounded-4 g-pa-15 g-pa-20--md g-mb-30">
