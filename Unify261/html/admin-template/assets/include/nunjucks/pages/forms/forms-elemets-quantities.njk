                      <input class="js-result form-control g-color-gray-dark-v6 g-brd-left g-brd-gray-light-v7 rounded-0 g-pa-9" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-left g-brd-gray-light-v7">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Round v.1 -->

                <!-- Round v.2 -->
                <div class="g-mb-30">
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Round v.2</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-140 g-brd-gray-light-v7 g-rounded-20">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-right g-brd-gray-light-v7">
                        <i class="hs-admin-minus"></i>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 rounded-0 g-pa-9" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11 g-brd-left g-brd-gray-light-v7">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Round v.2 -->

                <!-- Round v.3 -->
                <div>
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Round v.3</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-110 g-brd-none">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-30 g-height-30 g-font-size-10 g-color-gray-light-v11 g-brd-around g-brd-gray-light-v7 rounded-circle g-pa-5">
                        <i class="hs-admin-minus"></i>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 rounded-0 p-0" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-30 g-height-30 g-font-size-10 g-color-gray-light-v11 g-brd-around g-brd-gray-light-v7 rounded-circle g-pa-5">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Round v.3 -->
              </div>
              <!-- Round Quantities -->
            </div>
            <!-- End 2-nd column -->

            <!-- 3-d column -->
            <div class="col-md-4">
              <!-- Line Quantities -->
              <div class="g-brd-around g-brd-gray-light-v7 g-rounded-4 g-pa-15 g-pa-20--md g-mb-30">
                <h3 class="d-flex align-self-center text-uppercase g-font-size-12 g-font-size-default--md g-color-black g-mb-20">Line Quantities</h3>

                <!-- Line v.1 -->
                <div class="g-mb-30">
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Line v.1</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-185 g-brd-none g-brd-bottom g-brd-gray-light-v7 g-rounded-4">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-40 g-height-20 g-color-gray-light-v11 g-brd-right g-brd-gray-light-v7 g-my-10">
                        <i class="hs-admin-minus"></i>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 rounded-0 g-pa-9" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-40 g-height-20 g-color-gray-light-v11 g-brd-left g-brd-gray-light-v7 g-my-10">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Line v.1 -->

                <!-- Line v.2 -->
                <div class="g-mb-30">
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Line v.2</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-140 g-brd-none g-brd-bottom g-brd-gray-light-v7 g-rounded-4">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11">
                        <i class="hs-admin-minus"></i>
                      </div>
                      <input class="js-result form-control g-color-gray-dark-v6 rounded-0 g-pa-9" type="text" value="0" readonly>
                      <div class="js-plus input-group-addon d-flex align-items-center g-width-40 g-color-gray-light-v11">
                        <i class="hs-admin-plus"></i>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- End Line v.2 -->

                <!-- Line v.3 -->
                <div>
                  <h4 class="h6 g-font-weight-600 g-color-black g-mb-20">Line v.3</h4>

                  <div class="form-group mb-0">
                    <div class="js-quantity input-group u-quantity--v2 g-width-110 g-brd-none">
                      <div class="js-minus input-group-addon d-flex align-items-center g-width-30 g-height-30 g-font-size-10 g-color-gray-light-v11 g-pa-5">
